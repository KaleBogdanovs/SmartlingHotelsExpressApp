<div class="container">
    <div class="row" style="">
        <form class="center form-inline" role="form">
            <div class="form-group">
                <select class="form-control" id="selector" style="max-width: fit-content;">
                    <% for(var i = 0; i < locations.length; i++){ %>
                    <option value="<%= locations[i].link %>"><%= locations[i].txt %></option>
                    <% } %>
                </select>
            </div>
            <div class="form-group">
                <a id="findHotels" class="btn btn-primary" href="" role="button">Find Hotels</a></div>
            <div class="form-group">
                <a id="reviseSearch" class="btn btn-primary" href="/browse" role="button">Revise Search</a></div>
            <div class="form-group">
                <a id="returnToSearch" class="btn btn-primary" href="/browse/<%= hotel.prevLink %>" role="button">Return
                    to Search</a>
            </div>
        </form>
    </div>
</div>

<br>
